{
    "name": "melhorenvio/generator",
    "keywords": [
        "melhorenvio",
        "generator"
    ],
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Melhor Envio",
            "email": "dev@melhorenvio.com",
            "role": "Company"
        }
    ],
    "require": {
        "php": "^7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Melhorenvio\\Generator\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Melhorenvio\\Generator\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    }
}
